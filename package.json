{
	"name": "s-ui",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently 'pnpm:dev:*'",
		"dev:doc": "concurrently 'pnpm:watch:doc' 'pnpm:watch:tailwind-doc'",
		"dev:storybook": "concurrently 'pnpm:watch:storybook' 'pnpm:watch:tailwind-comp'",
		"watch:doc": "vite dev",
		"watch:storybook": "storybook dev -p 6006",
		"watch:tailwind-comp": "tailwindcss -c ./tailwind.comp.config.js -i ./src/lib/index.css -o ./src/lib/global.css --watch",
		"watch:tailwind-doc": "tailwindcss -c ./tailwind.doc.config.js -i ./src/app.css -o ./src/static/global.css --watch",
		"help": "tailwindcss --help",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"package": "svelte-package --output ./package",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test": "vitest",
		"build-storybook": "storybook build",
		"chromatic": "npx chromatic --project-token=chpt_41caf10e35cdde6",
		"prettier:format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^1.3.3",
		"@jest/globals": "^29.7.0",
		"@playwright/test": "^1.28.1",
		"@storybook/addon-designs": "^8.0.2",
		"@storybook/addon-essentials": "^8.0.9",
		"@storybook/addon-interactions": "^8.0.9",
		"@storybook/addon-links": "^8.0.9",
		"@storybook/blocks": "^8.0.9",
		"@storybook/manager-api": "^8.1.11",
		"@storybook/svelte": "^8.0.9",
		"@storybook/sveltekit": "^8.0.9",
		"@storybook/test": "^8.0.9",
		"@storybook/theming": "^8.1.11",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.3.1",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@testing-library/svelte": "^5.1.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.19",
		"chromatic": "^11.5.4",
		"concurrently": "^8.2.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-storybook": "^0.8.0",
		"eslint-plugin-svelte": "^2.35.1",
		"jsdom": "^24.0.0",
		"mdsvex": "^0.11.2",
		"postcss": "^8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"storybook": "^8.0.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwind-merge": "^2.4.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.2.0"
	},
	"type": "module"
}
